{
  "manifest_version": 2,
  "name": "BOOMERang",
  "version": "1.0",
  "icons": {
    "128": "jaba.png"
  },
  "browser_action": {
    "default_icon": "jaba.png",
    "default_popup": "./dist/popup.html"
  },
  "permissions": ["activeTab"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  }
}